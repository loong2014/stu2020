<?xml version="1.0" encoding="utf-8"?>
<com.sunny.lib.ui.SunnyConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.sunny.lib.ui.SunnyLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.sunny.lib.ui.SunnyButton
            android:id="@+id/btn_jump_stu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Stu" />

        <com.sunny.lib.ui.SunnyButton
            android:id="@+id/btn_jump_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="跳转首页" />

        <com.sunny.lib.ui.SunnyButton
            android:id="@+id/btn_jump_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="跳转登陆页" />

        <com.sunny.lib.ui.SunnyButton
            android:id="@+id/btn_jump_layout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="跳转布局页" />

        <com.sunny.lib.ui.SunnyButton
            android:id="@+id/btn_jump_web"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="跳转WebView页" />

        <com.sunny.lib.ui.SunnyButton
            android:id="@+id/btn_jump_weather"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="跳转Weather页" />

    </com.sunny.lib.ui.SunnyLinearLayout>

</com.sunny.lib.ui.SunnyConstraintLayout>
